<definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://example.com/soap"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             targetNamespace="http://example.com/soap"
             name="Service">

    <types>
        <xsd:schema targetNamespace="http://example.com/soap">
            <xsd:element name="getRequest" type="xsd:string"/>
            <xsd:element name="getResponse" type="xsd:string"/>
        </xsd:schema>
    </types>

    <message name="getRequest">
        <part name="request" element="tns:getRequest"/>
    </message>
    <message name="getResponse">
        <part name="response" element="tns:getResponse"/>
    </message>

    <portType name="ServicePortType">
        <operation name="getOperation">
            <input message="tns:getRequest"/>
            <output message="tns:getResponse"/>
        </operation>
    </portType>

    <binding name="ServiceSoapBinding" type="tns:ServicePortType">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="getOperation">
            <soap:operation soapAction="http://example.com/soap/getOperation"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>

    <service name="Service">
        <port name="ServiceSoapPort" binding="tns:ServiceSoapBinding">
            <soap:address location="http://localhost:8080/ws"/>
        </port>
    </service>
</definitions>
